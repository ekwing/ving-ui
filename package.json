{
  "name": "ving-ui",
  "version": "0.0.1",
  "author": "ekwing",
  "description": "mobile component library",
  "main": "lib/ving-ui.common.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "start-storybook -s ./storybook/public -c ./storybook/.storybook",
    "build": "rimraf lib && webpack --config build/webpack.component.js",
    "build:doc": "rimraf docs && build-storybook -s ./storybook/public -c ./storybook/.storybook -o docs",
    "lint": "eslint src/**/* packages/**/* examples/**/*.js examples/**/*.vue build/**/*.js --fix",
    "lint:style": "stylelint **/*.{html,vue,css,sass,scss} --fix"
  },
  "license": "MIT",
  "dependencies": {
    "dayjs": "^1.8.11",
    "popper.js": "^1.14.7",
    "resize-observer-polyfill": "^1.5.1",
    "throttle-debounce": "^2.1.0"
  },
  "peerDependencies": {
    "vue": "^2.6.6"
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@commitlint/cli": "^7.2.1",
    "@commitlint/config-conventional": "^7.1.2",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-viewport": "^5.0.6",
    "@storybook/addons": "^5.0.6",
    "@storybook/vue": "^5.0.6",
    "@types/popper.js": "^1.11.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__vue": "^5.0.1",
    "@types/webpack-env": "^1.13.9",
    "@vue/babel-preset-app": "^3.4.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "algoliasearch": "^3.24.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-vue": "^2.0.2",
    "css-loader": "^2.1.0",
    "eslint": "^5.16.0",
    "eslint-config-ekwing": "^0.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-simple-import-sort": "^3.1.0",
    "eslint-plugin-vue": "^5.2.2",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "husky": "^1.2.1",
    "lint-staged": "^7.2.2",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass": "^1.17.3",
    "sass-loader": "^7.1.0",
    "storybook-addon-vue-info": "^1.0.1",
    "style-loader": "^0.23.1",
    "stylelint": "^9.10.1",
    "stylelint-config-ekwing": "^0.1.0",
    "stylelint-webpack-plugin": "^0.10.5",
    "terser-webpack-plugin": "^1.2.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "typescript": "^3.4.1",
    "url-loader": "^1.1.2",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.0",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.5",
    "webpack-chain": "^5.2.0",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.0",
    "webpack-merge": "^4.2.1"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{html,vue,css,sass,scss}": [
      "stylelint --fix",
      "git add"
    ]
  }
}
